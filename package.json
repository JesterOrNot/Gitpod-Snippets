{
  "name": "GitpodUtil",
  "publisher": "JesterOrNot",
  "repository": "https://github.com/JesterOrNot/GitpodUtil",
  "displayName": "GitpodUtil",
  "description": "useful Gitpod Stufs",
  "version": "0.0.321",
  "engines": {
    "vscode": "1.43.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.gpinit"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "snippets": [
      {
        "language": "markdown",
        "path": "./snippets/markdown_snippets.json"
      },
      {
        "language": "yaml",
        "path": "./snippets/yaml_snippets.json"
      }
    ],
    "commands": [
      {
        "command": "extension.gpinit",
        "title": "GitpodUtil: Run gp init"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "7.1.1",
    "@types/mocha": "7.0.2",
    "@types/node": "12.12.30",
    "@types/vscode": "1.45.1",
    "eslint": "6.8.0",
    "@typescript-eslint/parser": "2.27.0",
    "@typescript-eslint/eslint-plugin": "2.27.0",
    "glob": "7.1.6",
    "mocha": "7.1.1",
    "typescript": "3.8.3",
    "vscode-test": "1.3.0"
  }
}
